{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "curly": false,
    "eofline": false,
    "forin": false,
    "max-line-length": [
      true,
      {
        "limit": 90,
        "ignore-pattern": "\/*\/"
      }
    ],
    "no-angle-bracket-type-assertion": false,
    "no-console": false,
    "no-unused-expression": false,
    "no-var-requires": false,
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "only-arrow-functions": false,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "any",
        "named-imports-order": "lowercase-first"
      }
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ]
  },
  "rulesDirectory": []
}